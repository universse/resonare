{
	"name": "palettez",
	"version": "0.0.23",
	"description": "Palettez",
	"keywords": ["theming"],
	"repository": {
		"type": "git",
		"url": "https://github.com/universse/palettez.git"
	},
	"license": "MIT",
	"author": "Si Phuoc <phuoc317049@gmail.com>",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"default": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/react.d.ts",
			"import": "./dist/react.js",
			"require": "./dist/react.cjs",
			"default": "./dist/react.js"
		},
		"./storage": {
			"types": "./dist/storage.d.ts",
			"import": "./dist/storage.js",
			"require": "./dist/storage.cjs",
			"default": "./dist/storage.js"
		},
		"./raw": {
			"import": "./dist/palettez.min.js"
		},
		"./global": {
			"types": "./global.d.ts"
		}
	},
	"jsdelivr": "./dist/palettez.min.js",
	"unpkg": "./dist/palettez.min.js",
	"types": "./dist/index.d.ts",
	"files": ["dist", "global.d.ts", "palettez.min.txt.d.ts"],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"size": "pnpm build && size-limit",
		"test": "vitest --reporter=basic --disable-console-intercept"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "11.2.0",
		"@types/react": "18.3.4",
		"jsdom": "26.1.0",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"size-limit": "11.2.0",
		"tsup": "8.4.0",
		"typescript": "5.5.4",
		"vitest": "3.1.2"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		}
	},
	"publishConfig": {
		"access": "public"
	}
}

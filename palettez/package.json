{
	"name": "palettez",
	"version": "0.0.30",
	"description": "Palettez",
	"keywords": [
		"theming"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/universse/palettez.git",
		"directory": "palettez"
	},
	"license": "MIT",
	"author": "Si Phuoc <phuoc317049@gmail.com>",
	"sideEffects": false,
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/react.d.ts",
			"import": "./dist/react.js",
			"default": "./dist/react.js"
		},
		"./storage": {
			"types": "./dist/storage.d.ts",
			"import": "./dist/storage.js",
			"default": "./dist/storage.js"
		},
		"./raw": {
			"import": "./dist/palettez.min.js"
		},
		"./global": {
			"types": "./global.d.ts"
		}
	},
	"jsdelivr": "./dist/palettez.min.js",
	"unpkg": "./dist/palettez.min.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist",
		"global.d.ts",
		"palettez.min.txt.d.ts"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"size": "pnpm build && size-limit",
		"test": "vitest"
	},
	"devDependencies": {
		"@size-limit/preset-small-lib": "11.2.0",
		"@types/react": "19.1.8",
		"jsdom": "26.1.0",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"size-limit": "11.2.0",
		"tsup": "8.5.0",
		"typescript": "5.8.3",
		"vitest": "3.2.4"
	},
	"peerDependencies": {
		"react": "^18.0.0 || ^19.0.0"
	},
	"peerDependenciesMeta": {
		"react": {
			"optional": true
		}
	},
	"publishConfig": {
		"access": "public"
	}
}
